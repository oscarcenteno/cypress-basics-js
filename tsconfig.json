{
  "compileOnSave": true,
  "compilerOptions": {
      "allowJs": true,
      "target": "ES2017",
      "experimentalDecorators": true,
      "checkJs": true,
      "noEmit": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "types": [
          "cypress",
          "node",
          "cypress-plugin-api",
          "cypress-plugin-steps"
      ]
  },
  "include": [
      "cypress/e2e/integration/**/*.js",
  ],
  "exclude": [
      "node_modules",
      "framework",
      "**/*.json"
  ]
}